def person: {
    def name: "Jensen"
    def speak: {
        print(name)
    }
    speak()
    name: "Bagelman"
    speak()
    name: "Joe"
}
print(person.name)    # prints "Joe"
person()              # prints "Jensen", then "Bagelman", then "Joe"
person.name: "Alfred" # changes def of name
print(person.name)    # prints "Joe"
person()              # prints "Alfred", then "Bagelman", then "Joe"
